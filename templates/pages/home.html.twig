	<div class="carousel-inner">
		<row>
			<col>
			{% set j=-1 %}
			{% for resource in front %}
			{% set j = j+1 %}
			<div {% if j == 0 %}class="carousel-item active" {% else %}class="carousel-item"{% endif %}>
			  {% if resource.filename %}
					<img class="d-block w-100 h-80" src="{{ vich_uploader_asset(resource, 'imageFile') | imagine_filter('carousel') }}" alt="" />
			  {% elseif resource.img %}
			  <img class="d-block w-100 h-80" src="http://cadic.cnd.fr/exl-img/{{ resource.img|replace({'.jpg': '_cadichd.jpg'}) }}" alt="" />
			  {% endif %}
				  <div class="carousel-caption d-md-block">
					<h5>{{ resource.title }}</h5>
					<p>{{ resource.type }}</p>
					<ul class="actions stacked">
						<li><a href="{{ path('resource.show', {slug: resource.slug, id: resource.id }) }}" class="button" style="color:#ffffff !important;box-shadow: inset 0 0 0 1px rgb(255, 255, 255);">Détails</a></li>
					</ul>
				  </div>
			</div>
			{% endfor %}
