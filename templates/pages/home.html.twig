				  <!-- One -->
				  <div id="dossiers"><p></p></div>
				  {% set k = -0 %}
				  {% for resource in foldersfront %}
					  {% set k = k+1 %}
					  {% if k is divisible by(2) %}
						  {% set orient = "orient-left" %}
					  {% else %}
					  {% set orient = "orient-right"%}
					  {% endif %}
					  <section class="spotlight style1 {{ orient }} content-align-left image-position-center onscroll-image-fade-in">
						  <div class="content">
							  <h2>{{ resource.title }}</h2>
							  <p>{{ resource.comment }}</p>
							  <ul class="actions stacked">
								  <li><a  href="{{ path('resource.show', {slug: resource.slug, id: resource.id }) }}"" class="button">Détails</a></li>
							  </ul>
						  </div>
						  {% if resource.filename %}
